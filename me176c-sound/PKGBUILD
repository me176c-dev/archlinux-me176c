# Maintainer: lambdadroid <lambdadroid@gmail.com>
pkgname=me176c-sound
pkgver=1
pkgrel=1
pkgdesc="Sound configuration files for the ASUS MeMO Pad 7 (ME176C/CX)"
arch=(any)
url="https://github.com/me176c-dev/me176c-archlinux"
license=('unknown')
depends=('alsa-utils')
options=('!strip')
source=(
    'bytcr-rt5640.conf'
    'HiFi'
)
sha256sums=('296ed4ef7cc41b03ba66abb5c17bde107b290e17acce468df347de2a882b16d9'
            'b45f4bd76ec79d2c57bcf42e996b469fc75eee008cecf44afe1a646453d6e57b')

package() {
    install -Dm644 bytcr-rt5640.conf "$pkgdir"/usr/share/alsa/ucm/bytcr-rt5640/bytcr-rt5640.conf
    install -Dm644 HiFi "$pkgdir"/usr/share/alsa/ucm/bytcr-rt5640/HiFi
}
