# Maintainer: lambdadroid <lambdadroid@gmail.com>
pkgname=me176c-bluetooth
pkgver=1
pkgrel=2
pkgdesc="Bluetooth configuration files for the ASUS MeMO Pad 7 (ME176C/CX)"
arch=(x86_64)
url="https://github.com/me176c-dev/me176c-archlinux"
license=('MIT')
depends=('bluez' 'bluez-utils' 'systemd' 'me176c-firmware')
options=('!strip')
source=('btattach-me176c.service')
sha256sums=('611d1558f234e91a65ffcf2d109eef0a3bc5e8f7b493a51bb4ff462b1c46eb6d')

package() {
    install -Dm644 btattach-me176c.service "$pkgdir"/usr/lib/systemd/system/btattach-me176c.service
}
