# Maintainer: lambdadroid <lambdadroid@gmail.com>
pkgname=me176c-thermal
pkgver=1
pkgrel=1
pkgdesc="Thermal configuration files for the ASUS MeMO Pad 7 (ME176C/CX)"
arch=(any)
url="https://github.com/me176c-dev/me176c-archlinux"
license=('MIT')
depends=('thermald')
options=('!strip')
source=(
    'thermald-me176c.service'
    'thermal-conf-me176c.xml'
)
sha256sums=('f509627121f1ad0efa2b8a2fa05cd92e39390f7f768061419b6a6c112399633e'
            'a0af6d78e878f79de6998c980816d0cb8e834a6a66fe92e7017a85da18277aee')

package() {
    install -Dm644 thermald-me176c.service "$pkgdir"/usr/lib/systemd/system/thermald-me176c.service
    install -Dm644 thermal-conf-me176c.xml "$pkgdir"/etc/thermald/thermal-conf-me176c.xml
}
